{
	"compilerOptions": {
		"baseUrl": ".",
		"paths": {
			"@/*": ["src/*"],
			"@assets/*": ["src/assets/*"],
			"@components/*": ["src/components/*"],
			"@configs/*": ["src/configs/*"],
			"@core/*": ["src/core/*"],
			"@data/*": ["src/data/*"],
			"@i18n/*": ["src/i18n/*"],
			"@layouts/*": ["src/layouts/*"],
			"@lib/*": ["src/lib/*"],
			"@styles/*": ["src/styles/*"],
			"@utils/*": ["src/utils/*"],
			"@atoms/*": ["src/components/atoms/*"]
		},
		"types": ["./worker-configuration.d.ts", "node"]
	},
	"extends": "astro/tsconfigs/strict",
	"include": [".astro/types.d.ts", "**/*", "./worker-configuration.d.ts"],
	"exclude": ["dist"]
}
