import { Editor } from 'slate';
import ReactEditorCommands from './partials/commands';
import ReactEditorEvents from './partials/events';
import ReactEditorQueries from './partials/queries';
import ReactEditorRendering from './partials/rendering';
import ReactEditorDomHelpers from './partials/dom-helpers';
/**
 * The `ReactEditor` class is a subclass of the `Editor` that adds React and
 * DOM specific behaviors and methods.
 */
declare class ReactEditor extends Editor {
}
interface ReactEditor extends ReactEditorCommands, ReactEditorDomHelpers, ReactEditorEvents, ReactEditorQueries, ReactEditorRendering {
}
export { ReactEditor };
