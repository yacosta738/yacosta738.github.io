/**
 * Find a native DOM selection point from a Slate `point`.
 *
 * @param {Point} point
 * @param {Window} win (optional)
 * @return {Object|Null}
 */
declare function findDOMPoint(point: any, win?: Window & typeof globalThis): {
    node: ChildNode;
    offset: number;
} | null;
/**
 * Export.
 *
 * @type {Function}
 */
export default findDOMPoint;
