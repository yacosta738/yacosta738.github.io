import { ReactEditor } from '../editor';
/**
 * Commands for the `ReactEditor`.
 */
export default class ReactEditorCommands {
    /**
     * Blur the editor.
     */
    blur(this: ReactEditor): void;
    /**
     * Focus the editor.
     */
    focus(this: ReactEditor): void;
    /**
     * Deselect the editor.
     */
    deselect(this: ReactEditor): void;
}
