export declare type NativeComment = Comment;
export declare type NativeElement = Element;
export declare type NativeNode = Node;
export declare type NativePoint = {
    node: NativeNode;
    offset: number;
};
export declare type NativeRange = Range;
export declare type NativeSelection = Selection;
export declare type NativeStaticRange = StaticRange;
/**
 * Check if a value is a DOM node.
 */
export declare const isNativeNode: (value: any) => value is Node;
/**
 * Check if a DOM node is an element node.
 */
export declare const isNativeElement: (value: any) => value is Element;
/**
 * Check if a DOM node is a comment node.
 */
export declare const isNativeComment: (value: any) => value is Comment;
