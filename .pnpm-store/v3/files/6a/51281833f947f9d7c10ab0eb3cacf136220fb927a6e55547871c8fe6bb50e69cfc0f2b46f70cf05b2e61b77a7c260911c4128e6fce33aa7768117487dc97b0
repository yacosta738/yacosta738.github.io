import { Editor, Annotation } from '../..';
declare class AnnotatingCommands {
    /**
     * Add a new `annotation` object with a `key`.
     */
    addAnnotation(this: Editor, key: string, annotation: Annotation): void;
    /**
     * Remove an existing annotation object by `key`.
     */
    removeAnnotation(this: Editor, key: string): void;
    /**
     * Set new properties on an annotation object with `key`.
     */
    setAnnotation(this: Editor, key: string, props: Partial<Annotation>): void;
}
export default AnnotatingCommands;
//# sourceMappingURL=annotating.d.ts.map