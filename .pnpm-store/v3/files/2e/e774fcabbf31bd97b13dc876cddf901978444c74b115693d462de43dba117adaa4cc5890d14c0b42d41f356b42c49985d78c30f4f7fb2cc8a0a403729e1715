import { Editor, Mark, Location } from '../..';
declare class MarkCommands {
    /**
     * Add a set of marks to the text nodes at a location.
     */
    addMarks(this: Editor, marks: Mark[], options?: {
        at?: Location;
        hanging?: boolean;
    }): void;
    removeMarks(this: Editor, marks: Mark[], options?: {
        at?: Location;
        hanging?: boolean;
    }): void;
    setMarks(this: Editor, marks: Mark[], props: Partial<Mark>, options?: {
        at?: Location;
        hanging?: boolean;
    }): void;
    toggleMarks(this: Editor, marks: Mark[], options?: {
        at?: Location;
        hanging?: boolean;
    }): void;
}
export default MarkCommands;
//# sourceMappingURL=mark.d.ts.map