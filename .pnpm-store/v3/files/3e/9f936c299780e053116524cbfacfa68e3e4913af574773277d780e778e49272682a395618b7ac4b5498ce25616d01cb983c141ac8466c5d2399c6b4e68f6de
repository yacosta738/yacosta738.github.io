import { Node } from '..';
/**
 * The `Fragment` interface describes a tree of nodes that exist independent of
 * a Slate document.
 */
interface Fragment {
    nodes: Node[];
    [key: string]: any;
}
declare namespace Fragment {
    /**
     * Check if a value implements the `Fragment` interface.
     */
    const isFragment: (value: any) => value is Fragment;
}
export { Fragment };
