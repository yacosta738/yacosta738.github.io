import { Editor, Range } from '../..';
declare class AnnotationCommands {
    /**
     * Add a new `annotation` object with a `key`.
     */
    addAnnotation(this: Editor, key: string, annotation: Range): void;
    /**
     * Remove an existing annotation object by `key`.
     */
    removeAnnotation(this: Editor, key: string): void;
    /**
     * Set new properties on an annotation object with `key`.
     */
    setAnnotation(this: Editor, key: string, props: Partial<Range>): void;
}
export default AnnotationCommands;
//# sourceMappingURL=annotation.d.ts.map