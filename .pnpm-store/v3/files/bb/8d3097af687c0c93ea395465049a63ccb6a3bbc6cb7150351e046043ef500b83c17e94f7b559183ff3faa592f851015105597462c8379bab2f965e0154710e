import { Editor } from '../..';
declare class GeneralQueries {
    /**
     * Check if the editor's selection is expanded.
     */
    isExpanded(this: Editor): boolean;
    /**
     * Check if the editor's selection is expanded.
     */
    isCollapsed(this: Editor): boolean;
    /**
     * Check if the editor is selected.
     */
    isSelected(this: Editor): boolean;
}
export default GeneralQueries;
