import { Editor, Path, Range, Point } from '../..';
declare class DeletingCommands {
    delete(this: Editor, options?: {
        at?: Path | Point | Range;
        distance?: number;
        unit?: 'character' | 'word' | 'line' | 'block';
        reverse?: boolean;
    }): void;
}
export default DeletingCommands;
