{
  "name": "yacosta-portfolio-monorepo",
  "private": true,
  "scripts": {
    "dev": "pnpm --filter=./apps/* --parallel dev",
    "build": "pnpm --filter=./apps/* --parallel build",
    "test": "pnpm --filter=./apps/* test",
    "start": "pnpm --filter=./apps/* --parallel start",
    "deploy": "pnpm --filter=./apps/* --parallel deploy",
    "ruler:apply": "npx @intellectronica/ruler apply",
    "ruler:check": "npx @intellectronica/ruler check"
  }
}
